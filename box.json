{
    "alias": "bolt",
    "chmod": "0755",
    "compactors": [
        "Herrera\\Box\\Compactor\\Json",
        "Herrera\\Box\\Compactor\\Php"
    ],
    "compression": "GZ",
    "directories": [
        "src",
        "web"
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": [
                ".gitignore",
                ".md",
                "phpunit",
                "Tester",
                "Tests",
                "tests",
                "yaml"
            ],
            "in": [
                "vendor"
            ]
        }
    ],
    "git-version": "package_version",
    "main": "bin\/bolt",
    "output": "bolt.phar",
    "stub": true,
    "web": false
}
